<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.nt.nothing.mapper.CardMapper">

    <select id="getBalanceByLevel" resultType="java.lang.Double">
        select balance from card_balance where level = #{level}
    </select>

    <select id="getCardBalanceResult" resultType="com.nt.nothing.pojo.CardResult">
        select card_balance.image,card_balance.name,card_balance.level from card_balance
        where level = #{level}
    </select>
</mapper>